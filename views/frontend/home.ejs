        }


        function showModal(message) {
            const modalMessage = document.getElementById('modal-message');
            modalMessage.textContent = message;
            errorMessage.style.display = 'block';
        }

        function closeModal() {
            errorMessage.style.display = 'none';
        }
        function okButtonAlertModal(){
            window.location.reload();
 
        }

        function validateSignupForm() {
            const form = document.getElementById('signupForm');
            const username = document.getElementById('username');
            const email = document.getElementById('reguseremail');
            const password = document.getElementById('register-password');



            let isValid = true;
            const nameError = document.getElementById('nameError');
            const emailError = document.getElementById('emailError');
            const passwordError = document.getElementById('passwordError');



            // Clear previous validation states
            form.classList.remove('was-validated');
            username.classList.remove('is-invalid');
            email.classList.remove('is-invalid');
            password.classList.remove('is-invalid');

            emailError.textContent = '';
            nameError.textContent = '';
            passwordError.textContent = '';


            // Validate username
            if (username.value.trim() === '') {
                username.classList.add('is-invalid');
                nameError.textContent = 'Name is required';
                isValid = false;
            } else {
                username.classList.remove('is-invalid');
                username.classList.add('is-valid');
            }

            // Validate email
            const emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
            if (!emailPattern.test(email.value)) {
                email.classList.add('is-invalid');
                document.getElementById('emailError').textContent = 'Enter a valid email';

                isValid = false;
            } else {
                email.classList.remove('is-invalid');
                email.classList.add('is-valid');
            }

            // Validate password
            if (password.value.trim() === '') {
                password.classList.add('is-invalid');
                passwordError.textContent = 'Password is required';
                isValid = false;
            }
            else {
                const passwordPattern = /^(?=.*[A-Za-z])(?=.*\d)[A-Za-z\d]{6,}$/;
                if (!passwordPattern.test(password.value)) {
                    password.classList.add('is-invalid');
                    passwordError.textContent = 'Password must be at least 6 characters long and contain at least one letter and one number.';
                    isValid = false;
                } else {
                    password.classList.remove('is-invalid');
                    password.classList.add('is-valid');
                    //password.nextElementSibling.innerText = '';
                }
            }

            // Add the 'was-validated' class to the form to trigger Bootstrap's validation styles
            form.classList.add('was-validated');


            return isValid;
        }

        function sendOTP() {

            const email = document.getElementById('reguseremail').value;


            fetch('/otp/sentotp', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ email: email })
            })
                .then(response => {
                    if (response.ok) {
                        document.getElementById('otpSection').style.display = 'block';

                        startTimer();
                        showModal('OTP sent successfully');
                        console.log('OTP sent successfully');

                    } else {
                        throw new Error('user you entered is already exist');
                    }
                })
                .catch(error => {
                    document.getElementById('signupBtn').style.display = 'block';
                    document.getElementById('otpSection').style.display = 'none';
                    showModal(error.message);
                    console.error('Error sending OTP:', error);
                });
        }

        function verifyOTP() {
            const email = document.getElementById('reguseremail').value;
            const otp = document.getElementById('reg-otp').value;
            const password = document.getElementById('register-password').value;
            const name = document.getElementById('username').value;



            fetch('otp/verifyotp', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ otp: otp, email: email, password: password, name: name })
            })
                .then(response => {
                    console.log("otp verification", response)
                    return response.json()
                }

                )
                .then(data => {
                    console.log(data)

                    if (data.error) {
                        // Display the error message
                        showModal(data.error);

                    } else {
                        if (data.verified) {
                            document.getElementById('okBtn').style.display = 'block';
                            document.getElementById('closeBtnalert').style.display = 'block';
                            showModal(data.message);
                        }
                        else {
                            document.getElementById('closeBtnalert').style.display = 'block';
                            showModal(data.message);
                        }

                    }
                })
                .catch(error => {
                    showModal(error.message);
                    console.error('Error verifying OTP:', error);
                });
        }

