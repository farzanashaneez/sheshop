        const resendOtpBtn = document.getElementById('resend-otp-btn-password');
        const resetPasswordBtn = document.getElementById('reset-password-btn');
        let countdownInterval;
        const errorMessage = document.getElementById('alertmodal');
        const signinForm = document.getElementById('signinForm');



        function handleForgotPassword() {

            // const resettab=document.getElementById('reset-tab');
            // resettab.style.display = 'block';

            // resetTab.setAttribute('aria-selected', 'true');

            passwordResetForm.style.display = 'block';
            document.getElementById('signin').classList.remove('show', 'active');
            document.getElementById('password-reset').classList.add('show', 'active');

            // Add event listener to the password reset form
            // passwordResetForm.addEventListener('submit', handlePasswordReset);
        }

        function handlePasswordReset(event) {
            console.log("handlePasswordReset")
            event.preventDefault();
            const email = document.getElementById('reset-email').value;

            // Send a request to the server to generate an OTP and send it to the user's email
            fetch('/otp/sendResetOtp', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ email })
            })
                .then(response => response.json())
                .then(data => {
                    console.log("sendResetOtp", data)
                    if (data.success) {
                        showModal(data.message);
                        document.getElementById("verify-email-btn").style.display='none';
                        otpContainer.style.display = 'block';
                        resendOtpBtn.disabled = false;
                        startCountdown();
                    } else {
                        // Display an error message
                        showModal(data.message);
                    }
                })
                .catch(error => {
                    console.error('Error resetting password:', error);
                    alert('An error occurred while resetting your password. Please try again later.');
                });
        }

        function verifyOtp() {
            console.log("in verifyOtp")
            const otp = document.getElementById('otp-input').value;
            const email = document.getElementById('reset-email').value;


            // Send a request to the server to verify the OTP
            fetch('otp/verifyResetOTP', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ otp, email })
            })
                .then(response => response.json())
                .then(data => {
                    if (data.verified) {
                        otpContainer.style.display = 'none';
                        // email.readOnly = true;
                        newPasswordContainer.style.display ='block';

                        // resetPasswordBtn.style.display = 'block';
                        resendOtpBtn.disabled = true;
                        clearCountdown();
                    } else {
                        // Display an error message
                        alert(data.message);
                    }
                })
                .catch(error => {
                    console.error('Error verifying OTP:', error);
                    alert('An error occurred while verifying the OTP. Please try again later.');
                });
        }

        function resetPassword() {

            if (!validateResetPassword()) {
                showModal("Password must be at least 6 characters long and contain at least one letter and one number.")
            }
            else {
                const newPassword = document.getElementById('new-password').value;
                const confirmPassword = document.getElementById('new-password-confirm').value;
                const email = document.getElementById('reset-email').value;

                if (newPassword !== confirmPassword) {
                    showModal("password confirmation failed..!")
                }

                else {
                    fetch('/otp/resetpassword', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({ newPassword, email })
                    })
                        .then(response => response.json())
                        .then(data => {
                            if (data.success) {
                                // Redirect the user to the login page or display a success message
                                document.getElementById('okBtn').style.display='block';
                                showModal('Password reset successful. Please log in with your new password.');
                                // Redirect to login page
                            } else {
                                document.getElementById('okBtn').style.display='block';
                                showModal('Password reset successful. Please log in with your new password.');
                            }
                        })
                        .catch(error => {
                            console.error('Error resetting password:', error);
                            showModal('An error occurred while resetting your password. Please try again later.');
                        });
                }
            }
        }

        function startCountdown() {
            let countdown = 120; // 2 minutes in seconds
            resendOtpBtn.disabled = true;

            countdownInterval = setInterval(() => {
                const minutes = Math.floor(countdown / 60);
                const seconds = countdown % 60;
                resendOtpBtn.textContent = `Resend OTP (${minutes}:${seconds.toString().padStart(2, '0')})`;

                if (--countdown < 0) {
                    clearCountdown();
                    resendOtpBtn.disabled = false;
                    resendOtpBtn.textContent = 'Resend OTP';
                }
            }, 1000);
        }

        function clearCountdown() {
            clearInterval(countdownInterval);
        }

        passwordResetForm.addEventListener('submit', handlePasswordReset);
        resendOtpBtn.addEventListener('click', () => handlePasswordReset(new Event('submit')));


        function validateResetPassword() {
            const newPassword = document.getElementById('new-password');
            if (newPassword.value.trim() === '') {
                return false;
            }
            else {
                const passwordPattern = /^(?=.*[A-Za-z])(?=.*\d)[A-Za-z\d]{6,}$/;
                if (!passwordPattern.test(newPassword.value)) {
                    return false;
                } else {
                    return true;
                }
            }
        }






        document.getElementById('signupBtn').addEventListener('click', function (event) {
            event.preventDefault(); // Prevent form submission
            console.log('signupBtn clicked')
            if (validateSignupForm()) {

                this.style.display = 'none';
                // document.getElementById('otpSection').style.display = 'block';
                sendOTP();
            }
        });

        document.getElementById('verifyOtpBtn').addEventListener('click', function () {
            verifyOTP();

        });

        document.getElementById('okBtn').addEventListener('click', function () {
            window.location.reload(); // Refresh the homepage
        });

        let countdown;
        let timer = 120; // 2 minutes in seconds
        function startTimer() {
            let minutes, seconds;
            countdown = setInterval(function () {
                minutes = parseInt(timer / 60, 10);
                seconds = parseInt(timer % 60, 10);

                minutes = minutes < 10 ? "0" + minutes : minutes;
                seconds = seconds < 10 ? "0" + seconds : seconds;

                document.getElementById("countdown").textContent = minutes + ":" + seconds;

                if (--timer < 0) {
                    clearInterval(countdown);
                    document.getElementById("resend-otp-btn").removeAttribute("disabled");
                    document.getElementById("countdown").textContent = "";
                }


            }, 1000);
        }

        document.getElementById("resend-otp-btn").addEventListener("click", function () {
            // Simulate OTP resend functionality
            console.log("Resending OTP...");
            sendOTP();
            // Disable the button and start the timer
            this.setAttribute("disabled", true);
            timer = 120; // Reset the timer
            startTimer();
        });



        // const errorMessage = document.getElementById('alertmodal');

        signinForm.addEventListener('submit', (event) => {
            event.preventDefault();

            const email = signinForm.querySelector('#useremail').value;
            const password = signinForm.querySelector('#userpassword').value;
            const closeButton = document.querySelector('#alertmodal .close');
            closeButton.addEventListener('click', closeModal);
            // const modalMessage = document.getElementById('modal-message');

            fetch('/home/signin', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ email, password })
            })
                .then(response => response.json())
                .then(data => {
                    console.log(data)

                    if (data.error) {
                        // Display the error message
                        showModal(data.error);

                    } else {
                        // Redirect the user or perform other actions
                        window.location.href = '/home';
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        });



        function addToCart(productId, user) {
